<div class="holder">
  <div class="header"><img src="inboard_logo.png" /></div><br />
  <div class="header"><h2>Admin Order Tracking</h2></div><br />
  <div class="wrapper">
    <hr />
    <div class="label"><h3>Search</h3></div><br />
    <form id="input" onsubmit="return false">
      <div class="child">Deck S/N</div>
      <div class="child">
        <input type="text" id="deck-sn" name="deck_sn" value="" style="width:100px">
      </div>
      <div style="width:75px; margin-left:10px;">
        <input class="button" id="user-submit" type="submit" value="Search">
      </div>
      <div style="width:75px;">
        <input class="button" id="user-clear" type="submit" value="Clear">
      </div><br />
    </form>
    <hr />
  </div><br />
  <div class="label"><h3>Results</h3></div><br />
  <div class="wrapper">
    <div class="child">Rush Rec ID</div><div class="child-sm"><input type="text" id="rrecid" class="child-tiny rush"></div>
    <div class="child">Data Rec ID</div><div class="child-sm2"><input type="text" id="drecid" class="child-tiny"></div>
    <div class="child">&nbsp;Item</div><div class="child-wide"><input class="rush" type="text" id="item"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">MFG Date</div><div class="child-wide"><input type="text" id="mfgdate"></div>
    <div class="child">&nbsp;Ship Date</div><div class="child-wide"><input class="rush" type="text" id="shipdate"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Motor S/N L</div><div class="child-wide"><input type="text" id="motorsnl"></div>
    <div class="child">&nbsp;Motor S/N R</div><div class="child-wide"><input type="text" id="motorsnr"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Motor Code</div><div class="child-wide"><textarea rows="3" cols="36" id="motor_failure_code"></textarea></div>
    <div class="child">&nbsp;Motor Comm</div><div class="child-wide"><textarea rows="3" cols="36"  id="motor_comments"></textarea></div>
  </div><br />
  <div class="wrapper">
    <div class="child">MA S/N</div><div class="child-wide"><input type="text" id="main_board_sn"></div>
    <div class="child">&nbsp;Battery S/N</div><div class="child-wide"><input type="text" id="battery_sn"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">MA Code</div><div class="child-wide"><textarea rows="3" cols="36" id="ma_failure_code"></textarea></div>
    <div class="child">&nbsp;BA Code</div><div class="child-wide"><textarea rows="3" cols="36" id="battery_failure_code"></textarea></div>
  </div><br />
  <div class="wrapper">
    <div class="child">MA Comments</div><div class="child-wide"><textarea id="ma_comments" rows="3" cols="36"></textarea></div>
    <div class="child">&nbsp;BA<br />&nbsp;Comments</div><div class="child-wide"><textarea id="battery_comments" rows="3" cols="36"></textarea></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Remote S/N</div><div class="child-wide"><input type="text" id="remote_sn"></div>
    <div class="child">&nbsp;In Part</div><div class="child-sm" style="width:40px"><input class="rush child-tiny" style="width:30px" type="text" id="inpart"></div>
    <div class="child">&nbsp;SPC Code</div><div class="child-sm2"><input class="rush child-tiny" style="width:100px" type="text" id="spc_code"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">FW Version</div><div class="child-wide"><input type="text" id="fwversion"></div>
    <div class="child">&nbsp;BCU Version</div><div class="child-wide"><input type="text" id="bcuversion"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Netsuite #</div><div class="child-wide"><input class="rush" type="text" id="netsuite"></div>
    <div class="child">&nbsp;Order #</div><div class="child-wide"><input class="rush" type="text" id="ordernum"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Internal Id</div><div class="child-wide"><input class="rush" type="text" id="internalid"></div>
    <div class="child">&nbsp;Tracking #</div><div class="child-wide"><input class="rush" type="text" id="trackingnu"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">First Name</div><div class="child-wide"><input class="rush" type="text" id="firstname"></div>
    <div class="child">&nbsp;Last Name</div><div class="child-wide"><input type="text" class="rush" id="lastname"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Address</div><div class="child-wide"><input class="rush" type="text" id="addr"></div>
    <div class="child">&nbsp;Address 2</div><div class="child-wide"><input class="rush" type="text" id="addr2"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">City</div><div class="child-wide"><input class="rush" type="text" id="city"></div>
    <div class="child">&nbsp;State</div><div class="child"><input class="rush" type="text" id="state"></div><br />
  </div><br />
  <div class="wrapper">
    <div class="child">Zip</div><div class="child-wide"><input class="rush" type="text" id="zipcode"></div>
    <div class="child">&nbsp;Country</div><div class="child"><input class="rush" type="text" id="cntry"></div><br />
  </div><br />
  <div class="wrapper">
    <div class="child">Email</div><div class="child-wide"><input class="rush" type="text" id="email"></div>
    <div class="child">&nbsp;Phone</div><div class="child-wide"><input class="rush" type="text" id="phone"></div><br />
  <div class="wrapper">
    <div class="child">Ship F Name</div><div class="child-wide"><input type="text" id="ship_first"></div>
    <div class="child">&nbsp;Ship L Name</div><div class="child-wide"><input type="text" id="ship_last"></div><br />
  </div><br />
  <div class="wrapper">
    <div class="child">Ship Addr</div><div class="child-wide"><input class="rush" type="text" id="ship_addr"></div>
    <div class="child">&nbsp;Ship Addr 2</div><div class="child-wide"><input class="rush" type="text" id="ship_addr2"></div>
  </div><br />
  <div class="wrapper">
    <div class="child">Ship City</div><div class="child-wide"><input class="rush" type="text" id="ship_city"></div>
    <div class="child">&nbsp;Ship State</div><div class="child"><input class="rush" type="text" id="ship_st"></div><br />
  </div><br />
  <div class="wrapper">
    <div class="child">Ship Zip</div><div class="child-wide"><input class="rush" type="text" id="ship_zip"></div>
    <div class="child">&nbsp;Ship Cntry</div><div class="child"><input class="rush" type="text" id="ship_ctry"></div><br />
  </div><br />
    <hr />
  </div><br />
